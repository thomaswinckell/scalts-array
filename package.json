{
  "name": "scalts-array",
  "version": "0.7.0",
  "description": "Array enrichment based on lodash for Typescript and scalts",
  "keywords": [
    "typescript",
    "es6",
    "ts",
    "scalts",
    "list",
    "array",
    "lodash",
    "enrichment",
    "functional"
  ],
  "author": "Thomas Winckell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomaswinckell/scalts-array.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts",
    "index.js",
    "index.d.ts",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf src/**/*.js && rm -rf src/**/*.d.ts && rm -rf test/**/*.js && rm -rf test/**/*.d.ts && rm index.js && rm index.d.ts",
    "prebuild": "npm run clean",
    "build": "tsc && babel ./src --out-dir ./src --ignore ./node_modules/",
    "pretest": "npm run build",
    "test": "istanbul cover -x '**/logger.*' _mocha test/**/*.js",
    "posttest": "npm run clean",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "@types/lodash": "4.*.*",
    "lodash": "4.*.*",
    "scalts": "1.1.*"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/power-assert": "^1.4.29",
    "@types/lodash": "^4.14.63",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codeclimate-test-reporter": "^0.4.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "power-assert": "^1.4.2",
    "scalts": "^1.1.0",
    "typescript": "^2.3.1"
  }
}
